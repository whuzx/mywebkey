            </intent-filter>
        </activity>
                       
        <activity android:name=".DynDnsSettings"        
                  android:label="@string/app_name"
                  android:launchMode="singleInstance">
        </activity>
        
        <activity android:name=".activity.LogView" 
        		  android:launchMode="singleInstance"        
                  android:label="@string/app_name">
        </activity>
        
        <activity android:name=".activity.ChatUI" android:configChanges="orientation"
        		android:launchMode="singleInstance" 
        		android:label="Webkey Chat" >
        		<intent-filter>
                	<action android:name="android.intent.action.MAIN" />
             	</intent-filter>
        </activity> 

        <activity android:name=".activity.RemoteScreenTab"
                        android:launchMode="singleInstance" 
                        android:label="Webkey Remote">
                <intent-filter>
                        <action android:name="android.intent.action.MAIN" />
                </intent-filter>
        </activity>

        <activity android:name=".remotescreen.RemoteQualitySettings"
                        android:launchMode="singleInstance" 
                        android:label="Webkey Remote Settings">
                <intent-filter>
                        <action android:name="android.intent.action.MAIN" />
                </intent-filter>
        </activity>

        <activity android:name=".remotescreen.RemoteScreenActivity"
                        android:launchMode="singleInstance" 
                        android:label="Webkey Remote"
                        android:screenOrientation="portrait"
                        android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
                <intent-filter>
                        <action android:name="android.intent.action.MAIN" />
                </intent-filter>
        </activity>


        <activity android:name=".remotescreen.RemoteScreenUtil"
                        android:launchMode="singleInstance"
                        android:screenOrientation="portrait"
                        android:label="Webkey">
                 <intent-filter>
                        <action android:name="android.intent.action.MAIN" />
                </intent-filter>
        </activity>


		<receiver android:name="ChatReceiver">  
                 <intent-filter>  
                        <action android:name="webkey.intent.action.Chat"/>    
                 </intent-filter>   
		</receiver>        
        
        <receiver android:name=".receivers.RemoteRegistrationReceiver">  
                 <intent-filter>  
                        <action android:name="webkey.intent.action.remote.registration"/>    
                 </intent-filter>   
		</receiver>        
        
                     
		<receiver android:name="BootRinstall" android:permission="android.permission.RECEIVE_BOOT_COMPLETED">  
                 <intent-filter>  
                         <action android:name="android.intent.action.BOOT_COMPLETED"/>  
                 </intent-filter>   
		</receiver>

		<receiver android:name=".GPS"> 
			<intent-filter> 
				<action android:name="webkey.intent.action.GPS.START" />
				<action android:name="webkey.intent.action.GPS.STOP" /> 
			</intent-filter> 
		</receiver>
		
		<receiver android:name=".SL4A"> 
			<intent-filter> 
				<action android:name="webkey.intent.action.SL4A.START" />
			</intent-filter> 
		</receiver>

		<receiver android:name=".SMS">
			<intent-filter>
				<action android:name="webkey.intent.action.SMS.SEND"/>
			</intent-filter>
		</receiver>
		
		<receiver android:name=".Widget" android:label="@string/app_name">                  
            <intent-filter>
            	<action android:name="android.appwidget.action.APPWIDGET_UPDATE" />
            </intent-filter>
            <meta-data
                android:name="android.appwidget.provider"
                android:resource="@xml/watch_widget_provider"/>
		</receiver>
				          
		<receiver androidenabled="true" android:name=".NetTest">                  
            <intent-filter>
    			<action android:name="android.net.conn.CONNECTIVITY_CHANGE"/>
    			<category android:name="android.intent.category.DEFAULT" />  
            </intent-filter>
		</receiver>
    </application>
     
