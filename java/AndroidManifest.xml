      <uses-feature android:name="android.hardware.wifi" android:required="false" />
      <uses-feature android:name="android.hardware.microphone" android:required="false" />
       
     <application android:icon="@drawable/icon" android:label="@string/app_name"
	android:name=".ScriptApplication">
      <service android:enabled="true" android:name=".ScriptService" />
        <activity android:name=".Webkey"
               		android:launchMode="singleInstance" 
               		android:label="@string/app_name"                		               		
               		>
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />             
            </intent-filter>
        </activity>
	<activity
		android:configChanges="keyboardHidden|orientation"
		android:name="com.googlecode.android_scripting.activity.FutureActivity" 
		android:theme="@android:style/Theme.Translucent"/>

        <activity android:name=".activity.WebkeyTab"
               		android:launchMode="singleInstance" 
               		android:label="@string/app_name"                		               		
               		>
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />        
            </intent-filter>
        </activity>
        
        <activity android:name=".activity.SettingsTab"
               		android:launchMode="singleInstance" 
               		android:label="@string/app_name"                		               		
               		>
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />        
            </intent-filter>
        </activity>     
           
        <activity android:name=".PreparePermissions" android:configChanges="orientation"
